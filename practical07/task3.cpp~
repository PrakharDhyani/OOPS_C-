
#include<iostream>
#include<ctime>
#include<string>
using namespace std;
int main(){
  int  arr[3][3];
  for(int i = 2 ;i >=0;i--)
  	{
  	    for(int j = 2 ; j>=0 ;j -- )
  	    {
  	        cin >> arr[i][j];
  	    } 
  	}
  	  for(int i = 2 ;i >=0;i--)
  	{
  	    for(int j = 2 ; j>=0 ;j -- )
  	    {
  	        cout << arr[i][j];
  	    } 
  	}
    return 0;
}
